<?xml version="1.0" encoding="ISO-8859-1"?>
<LibroCompraVenta xmlns="http://www.sii.cl/SiiDte" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sii.cl/SiiDte LibroCVS_v10.xsd" version="1.0">
<EnvioLibro ID="ID2005-01"> <!-- Chequear las caracteristicas que debe tener esta ID -->
        {{ caratula }}
        <ResumenPeriodo>
            {% for doc in resumen_periodos %}
            <TotalesPeriodo>
                <TpoDoc>doc.30</TpoDoc>
                <TotDoc>1</TotDoc> <!-- Cantidad de documentos del tipo especificado en el campo Tipo de Documento. Incluye anulados. -->
                <TotMntExe>doc.0</TotMntExe> <!-- TOTAL MONTO EXENTO O NO GRAVADO -->
                <TotMntNeto>doc.100</TotMntNeto> <!-- TOTAL MONTO NETO -->
                <TotOpIVARec>doc.1</TotOpIVARec> <!-- OPERACIONES CON IVA RECUPERABLE -->
                <TotMntIVA>doc.19</TotMntIVA> <!-- TOTAL MONTO IVA (recuperable) -->
                <TotMntTotal>doc.119</TotMntTotal> <!-- TOTAL MONTO TOTAL -->
            </TotalesPeriodo>
            {% endfor %}
        </ResumenPeriodo>
        {% for doc in detalles %}
        <Detalle>
            <TpoDoc>doc.30</TpoDoc> 
            <NroDoc>doc.1000000</NroDoc>
            <TasaImp>doc.19.0</TasaImp>
            <NumInt>doc.0</NumInt>
            <FchDoc>doc.2010-12-01</FchDoc>
            <RUTDoc>doc.14123166-9</RUTDoc>
            <RznSoc>doc.Rut de prueba</RznSoc>
            <MntExe>doc.0</MntExe>
            <MntNeto>doc.100</MntNeto>
            <MntIVA>doc.9</MntIVA>
            {% if otros_impuestos %}
            <OtrosImp>
                <CodImp>doc.14</CodImp>
                <TasaImp>doc.19.0</TasaImp>
                <MntImp>doc.133</MntImp>
            </OtrosImp>
            {% endif %}
            <MntTotal>doc.119</MntTotal>
        </Detalle>
        {% endfor %}
        <TmstFirma>2006-05-04T15:17:48</TmstFirma>
    </EnvioLibro>
</LibroCompraVenta>
