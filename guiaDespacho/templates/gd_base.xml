<?xml version="1.0" encoding="ISO-8859-1"?>
<EnvioDTE xmlns="http://www.sii.cl/SiiDte" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sii.cl/SiiDte EnvioDTE_v10.xsd" version="1.0">
<SetDTE ID="SetDoc">
	<Caratula version="1.0">
		<RutEmisor>{{compania.rut}}</RutEmisor>
		<RutReceptor>{{form.rut}}</RutReceptor> <!-- No lo esta tomando -->
		<FchResol>{{compania.fecha_resolucion}}</FchResol>
		<NroResol>{{compania.numero_resolucion}}</NroResol>
		<TmstFirmaEnv>{{ timestamp_firma }}</TmstFirmaEnv>
		<SubTotDTE>
			<TpoDTE>{{ folio.tipo_de_documento }}</TpoDTE>
			<NroDTE>1</NroDTE> <!-- No configurado en Conector -->
		</SubTotDTE>
	</Caratula>
<DTE version="1.0">
	<Documento ID="{{datos.numero_factura}}">
		<Encabezado>
			<IdDoc>
				<TipoDTE>{{ folio.tipo_de_documento }}</TipoDTE>
				<Folio>{{ instance.n_folio }}</Folio>
				<FchEmis>{% now "Y-m-d" %}</FchEmis>
				<FmaPago>{{datos.forma_pago}}1</FmaPago>
			</IdDoc>
			<Emisor>
				<RUTEmisor>{{compania.rut}}</RUTEmisor>
				<RznSoc>{{compania.razon_social}}</RznSoc>
				<GiroEmis>{{compania.giro}}</GiroEmis>
				<Acteco>{{compania.actividad_principal}}</Acteco>
				<DirOrigen>{{compania.direccion}}</DirOrigen>
				<CmnaOrigen>{{compania.comuna}}</CmnaOrigen>
			</Emisor>
			<Receptor>
				<RUTRecep>{{datos.rut}}</RUTRecep>
				<RznSocRecep>{{datos.senores}}</RznSocRecep>
				<GiroRecep>{{datos.giro}}</GiroRecep>
				<DirRecep>{{datos.direccion}}</DirRecep>
				<CmnaRecep>{{datos.comuna}}</CmnaRecep>
				<CiudadRecep>{{datos.ciudad_receptora}}</CiudadRecep>
			</Receptor>
			<Totales>
				<MntNeto>{{datos.neto}}</MntNeto>
				<TasaIVA>{{compania.tasa_de_iva}}</TasaIVA> 
				<IVA>{{datos.iva}}</IVA>
				<MntTotal>{{datos.total}}</MntTotal>
			</Totales>
		</Encabezado>
		<Detalle>
			<NroLinDet>{{forloop.counter}}</NroLinDet>
			<CdgItem>
				<TpoCodigo>INT1</TpoCodigo>
				<VlrCodigo>{{prod.item_code}}</VlrCodigo>
			</CdgItem>
			<NmbItem>{{prod.description}}</NmbItem>
			<DscItem/>
			<QtyItem>{{prod.qty}}</QtyItem>
			<PrcItem>{{prod.base_net_rate}}</PrcItem>
			<MontoItem>{{prod.amount}}</MontoItem>
		</Detalle>{%endfor%}
		<TED version="1.0">
		<DD>
			<RE>{{ folio.rut }}</RE>
			<TD>{{ folio.tipo_de_documento }}</TD>
			<F>{{ instance.n_folio }}</F>
			<FE>{{ instance.created|date:"Y-m-d" }}</FE>
			<RR>{{ data.rut }}</RR> 
			<RSR>{{ data.senores }}</RSR>
			<MNT>{{data.total}}</MNT>
			<IT1>{{data.primero}}</IT1>
			<CAF version="1.0">
				<DA>
					<RE>{{ folio.rut }}</RE>
					<RS>{{ folio.razon_social }}</RS>
					<TD>{{ folio.tipo_de_documento }}</TD>
					<RNG><D>{{ folio.rango_desde }}</D><H>{{ folio.rango_hasta }}</H></RNG>
					<FA>{{ folio.fecha_de_autorizacion|date:"Y-m-d" }}</FA>
					<RSAPK><M>{{ folio.pk_modulo }}</M><E>{{folio.pk_exponente}}</E></RSAPK>
					<IDK>{{ folio.idk }}</IDK>
				</DA>
				<FRMA algoritmo="SHA1withRSA">{{ folio.firma }}</FRMA>
			</CAF>
			<TSTED>{{ timestamp }}</TSTED>
		</DD>
		</TED>
		<TmstFirma>{{ timestamp_documento }}</TmstFirma>
	</Documento>
	<Signature xmlns="http://www.w3.org/2000/09/xmldsig#">
		<SignedInfo>
			<CanonicalizationMethod Algorithm="http://www.w3.org/TR/2001/REC-xml-c14n-20010315"/>
			<SignatureMethod Algorithm="http://www.w3.org/2000/09/xmldsig#rsa-sha1"/>
			<Reference URI="#{{form.numero_factura}}">
			<Transforms>
			<Transform Algorithm="http://www.w3.org/TR/2001/REC-xml-c14n-20010315"/>
			</Transforms>
			<DigestMethod Algorithm="http://www.w3.org/2000/09/xmldsig#sha1"/>
			<DigestValue></DigestValue>
			</Reference>
		</SignedInfo>
		<SignatureValue></SignatureValue>
		<KeyInfo>
			<KeyValue>
				<RSAKeyValue>
				<Modulus></Modulus>
				<Exponent></Exponent>
				</RSAKeyValue>
			</KeyValue>
			<X509Data>
			<X509Certificate></X509Certificate>
			</X509Data>
		</KeyInfo>
	</Signature>
</DTE></SetDTE>
<Signature xmlns="http://www.w3.org/2000/09/xmldsig#">
	<SignedInfo>
		<CanonicalizationMethod Algorithm="http://www.w3.org/TR/2001/REC-xml-c14n-20010315"/>
		<SignatureMethod Algorithm="http://www.w3.org/2000/09/xmldsig#rsa-sha1"/>
		<Reference URI="#{{form.numero_factura}}">
		<Transforms>
		<Transform Algorithm="http://www.w3.org/TR/2001/REC-xml-c14n-20010315"/>
		</Transforms>
		<DigestMethod Algorithm="http://www.w3.org/2000/09/xmldsig#sha1"/>
		<DigestValue></DigestValue>
		</Reference>
	</SignedInfo>
	<SignatureValue></SignatureValue>
	<KeyInfo>
		<KeyValue>
			<RSAKeyValue>
			<Modulus></Modulus>
			<Exponent></Exponent>
			</RSAKeyValue>
		</KeyValue>
		<X509Data>
		<X509Certificate></X509Certificate>
		</X509Data>
	</KeyInfo>
</Signature>
</EnvioDTE>
